{
  "name": "pravda-frontend",
  "engines": {
    "node": "22"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate-api": "orval --clean",
    "generate:unsafe": "pnpm generate-api && nuxt generate --dotenv .env.local",
    "generate": "pnpm generate:unsafe && pnpm lint",
    "preview": "nuxt preview",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "nuxt prepare",
    "lint": "tsc && vue-tsc --noEmit && eslint --cache \"{**/*,*}.{js,ts,vue}\"",
    "reformat-code": "eslint --fix \"./**/*.{js,ts,vue}\"",
    "knip": "knip"
  },
  "devDependencies": {
    "@element-plus/nuxt": "^1.1.1",
    "@nuxt/devtools": "2.3.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/node": "^22.13.10",
    "@typescript-eslint/parser": "^8.26.1",
    "eslint": "8.57.0",
    "eslint-config-hardcore": "github:BMFreed/eslint-config-hardcore",
    "eslint-plugin-nuxt": "^4.0.0",
    "knip": "^5.46.0",
    "prettier": "^3.5.3",
    "prettier-plugin-jsdoc": "^1.3.2",
    "type-fest": "^4.37.0",
    "typescript": "5.3.3",
    "vue-component-type-helpers": "^2.2.8",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.2.8"
  },
  "dependencies": {
    "@tanstack/vue-query": "^5.68.0",
    "@vueuse/core": "^12.8.2",
    "@vueuse/router": "^12.8.2",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.7",
    "element-plus": "^2.9.6",
    "normalize.css": "^8.0.1",
    "nuxt": "^3.16.0",
    "orval": "^7.7.0",
    "remeda": "^2.21.2"
  },
  "packageManager": "pnpm@10.0.0+sha512.b8fef5494bd3fe4cbd4edabd0745df2ee5be3e4b0b8b08fa643aa3e4c6702ccc0f00d68fa8a8c9858a735a0032485a44990ed2810526c875e416f001b17df12b"
}
