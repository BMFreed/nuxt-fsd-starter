{
  "name": "frontend-nuxt-starter",
  "engines": {
    "node": "21"
  },
  "browserslist": [
    "defaults and fully supports es6-module"
  ],
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "nuxt prepare",
    "lint": "tsc && vue-tsc --noEmit && eslint \"{**/*,*}.{js,ts,vue}\"",
    "lint-unused-modules": "eslint --config 'eslint/unusedModulesConfig.js' \"{**/*,*}.{js,ts,vue}\"",
    "reformat-code": "eslint --fix \"./**/*.{js,ts,vue}\""
  },
  "devDependencies": {
    "@element-plus/nuxt": "^1.0.9",
    "@nuxt/devtools": "^1.3.9",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/node": "^22.1.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "8.57.0",
    "eslint-config-hardcore": "^47.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-boundaries": "^4.2.2",
    "eslint-plugin-jsdoc": "^48.11.0",
    "eslint-plugin-nested-if": "^1.0.0",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.27.0",
    "prettier": "^3.3.3",
    "prettier-plugin-jsdoc": "^1.3.0",
    "type-fest": "^4.23.0",
    "typescript": "5.3.3",
    "vue-component-type-helpers": "^2.0.29",
    "vue-eslint-parser": "^9.4.3",
    "vue-tsc": "^2.0.29"
  },
  "dependencies": {
    "@vueuse/core": "^10.11.0",
    "deepmerge": "^4.3.1",
    "element-plus": "^2.7.8",
    "normalize.css": "^8.0.1",
    "nuxt": "^3.12.4",
    "remeda": "^2.7.0"
  }
}
