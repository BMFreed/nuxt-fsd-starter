{
  "name": "ibs-is-questionnaire",
  "engines": {
    "node": "21"
  },
  "browserslist": [
    "defaults and fully supports es6-module"
  ],
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "preinstall": "npx only-allow pnpm",
    "postinstall": "nuxt prepare",
    "lint": "tsc && vue-tsc --noEmit && eslint \"{**/*,*}.{js,ts,vue}\"",
    "lint-unused-modules": "eslint --config 'eslint/unusedModulesConfig.js' \"{**/*,*}.{js,ts,vue}\"",
    "reformat-code": "eslint --fix \"./**/*.{js,ts,vue}\""
  },
  "devDependencies": {
    "@nuxt/devtools": "^1.0.8",
    "@nuxtjs/eslint-config-typescript": "^12.0.0",
    "@types/node": "^20.11.25",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.46.0",
    "eslint-config-hardcore": "^45.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-boundaries": "^4.2.0",
    "eslint-plugin-jsdoc": "^48.2.3",
    "eslint-plugin-nested-if": "^1.0.0",
    "eslint-plugin-no-explicit-type-exports": "^0.12.1",
    "eslint-plugin-nuxt": "^4.0.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-sonar": "^0.13.2",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vue": "^9.16.1",
    "nuxt-primevue": "^0.3.1",
    "prettier": "^3.2.5",
    "prettier-plugin-jsdoc": "^1.3.0",
    "type-fest": "^4.18.2",
    "typescript": "^5.1.6",
    "vue-component-type-helpers": "^2.0.21",
    "vue-eslint-parser": "^9.3.1",
    "vue-tsc": "^2.0.6"
  },
  "dependencies": {
    "@vueuse/core": "^10.10.0",
    "deepmerge": "^4.3.1",
    "normalize.css": "^8.0.1",
    "nuxt": "^3.6.5",
    "primeicons": "^7.0.0",
    "primelocale": "github:primefaces/primelocale",
    "primevue": "^3.53.0",
    "remeda": "^1.24.0"
  },
  "version": "0.1",
  "author": "Биницкий Богдан",
  "license": "MIT"
}
